<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="Users">
    <indexes>
      <index name="fk_organization" columns="organization_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastname" type="string" column="lastname" length="255" nullable="false"/>
    <field name="firstname" type="string" column="firstname" length="255" nullable="false"/>
    <field name="middlename" type="string" column="middlename" length="255" nullable="false"/>
    <field name="birth" type="date" column="birth" nullable="false"/>
    <field name="tin" type="bigint" column="TIN" nullable="false"/>
    <field name="snils" type="integer" column="snils" nullable="false"/>
    <many-to-one field="organization" target-entity="Organizations">
      <join-columns>
        <join-column name="organization_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
